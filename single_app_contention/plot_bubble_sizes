#!/usr/local/bin/gnuplot

set title "Bubble Size (KB)"
set auto x
set yrange [0:12288]
set ylabel "Bubble Size (KB)"
set format y "%f"
set xtics rotate by 45
set xlabel "Benchmark"
set style histogram errorbars gap 2 lw 1
set style data histograms
set style fill solid border

set terminal pdf
set output 'bubble_size.pdf'
plot 'processed_data' using 1:4:3:xticlabels(9) title 'Bubble Size'

legend = 'mean median p95 p99'
set style histogram clustered
plot for [COL=1:4] 'processed_data' using COL:xticlabels(9) title word(legend, (COL + 1)/2)
